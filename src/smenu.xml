<?xml version="1.0"?>
<wf-panel-pi>
	<plugin name="panel">
	<_short>Panel</_short>
	<group>
	<_short>Searchable Menu</_short>
	<option name="smenu_padding" type="int">
		<_short>Icon Horizontal Padding</_short>
		<default>6</default>
	</option>
	<option name="smenu_show_tooltips" type="bool">
		<_short>Searchable Menu Show Tooltips</_short>
		<default>false</default>
	</option>
	<option name="smenu_search_height" type="int">
		<_short>Searchable Menu Search Window Height</_short>
		<default>300</default>
	</option>
	<option name="smenu_search_fixed" type="bool">
		<_short>Searchable Menu Fix Height of Search Window</_short>
		<default>false</default>
	</option>
	</group>
	</plugin>
</wf-panel-pi>
